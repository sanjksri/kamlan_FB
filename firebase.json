{
  "hosting": {
    "site": "kamlan",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "kamlan-fb",
          "region": "us-central1"
        }
      }
    ]
  }
}
