{
  "hosting": {
    "site": "kamlan",
    "public": "frontend/out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      { "source": "/api/**", "run": { "serviceId": "kamlan-api", "region": "us-central1" } },
      { "source": "**", "destination": "/index.html" }
    ]
  }
}
